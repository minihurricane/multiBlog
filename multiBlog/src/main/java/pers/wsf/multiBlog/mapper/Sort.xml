<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="pers.wsf.multiBlog.mapper.SortMapper">
	
	<select id="getSortId" parameterType="String" resultType="Integer">
		select sort_id from t_sort 
		where sort=#{sort}
	</select>
</mapper>